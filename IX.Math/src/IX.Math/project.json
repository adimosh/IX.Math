{
  "title": "IX.Math",
  "version": "0.3.2",

  "dependencies": {
    "System.Collections": "4.0.11",
    "System.Linq": "4.1.0",
    "System.Linq.Expressions": "4.1.0",
    "System.Runtime": "4.1.0",
    "System.Runtime.Extensions": "4.1.0",
    "System.Runtime.Serialization.Primitives": "4.1.1",
    "System.Text.RegularExpressions": "4.1.0",
    "System.Threading.Tasks": "4.0.11"
  },

  "frameworks": {
    "netstandard1.0": {
      "buildOptions": {
        "define": [ "NETSTANDARD10" ]
      },
      "imports": [
        "wp80",
        "wp81",
        "portable-netcore451+wp81",
        "portable-net45+wp8",
        "portable-net45+netcore45+wp8",
        "portable-wpa81+wp81",
        "portable-netcore451+wpa81+wp81",
        "portable-net45+netcore45+wpa81+wp8"
      ],
      "dependencies": {
        "System.Reflection.Extensions": "4.0.1"
      }
    },
    "netstandard1.1": {
      "buildOptions": {
        "define": [ "NETSTANDARD11" ]
      },
      "imports": [
        "net45",
        "win80",
        "portable-net45+netcore45",
        "portable-net45+netcore45+wpa81"
      ],
      "dependencies": {
        "System.Reflection.Extensions": "4.0.1",
        "System.Runtime.InteropServices": "4.1.0"
      }
    },
    "netstandard1.2": {
      "buildOptions": {
        "define": [ "NETSTANDARD12" ]
      },
      "imports": [
        "net451",
        "net452",
        "monoandroid",
        "monotouch",
        "monomac"
      ],
      "dependencies": {
        "System.Reflection.Extensions": "4.0.1",
        "System.Runtime.InteropServices": "4.1.0"
      }
    },
    "netstandard1.3": {
      "buildOptions": {
        "define": [ "NETSTANDARD13" ]
      },
      "imports": [
        "net46"
      ],
      "dependencies": {
        "System.Reflection.TypeExtensions": "4.1.0",
        "System.Runtime.InteropServices": "4.1.0"
      }
    },
    "netstandard1.4": {
      "buildOptions": {
        "define": [ "NETSTANDARD14" ]
      },
      "imports": [
        "net461",
        "uap10"
      ],
      "dependencies": {
        "System.Reflection.TypeExtensions": "4.1.0",
        "System.Runtime.InteropServices": "4.1.0"
      }
    },
    "netstandard1.5": {
      "buildOptions": {
        "define": [ "NETSTANDARD15" ]
      },
      "imports": [
        "net462"
      ],
      "dependencies": {
        "System.Reflection.TypeExtensions": "4.1.0",
        "System.Runtime.InteropServices": "4.1.0"
      }
    },
    "netstandard1.6": {
      "buildOptions": {
        "define": [ "NETSTANDARD16" ]
      },
      "imports": [
        "netcoreapp10",
        "net463",
        "dnxcore50",
        "xamarinios",
        "xamarinpsthree",
        "xamarinpsfour",
        "xamarinpsvita",
        "xamarinwatchos",
        "xamarintvos",
        "xamarinxboxthreesixty",
        "xamarinxboxone"
      ],
      "dependencies": {
        "System.Reflection.TypeExtensions": "4.1.0",
        "System.Runtime.InteropServices": "4.1.0"
      }
    }
  },

  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [
          "DEBUG"
        ]
      }
    },
    "Release": {
      "buildOptions": {
        "warningsAsErrors": true,
        "optimize": true
      }
    }
  },

  "buildOptions": {
    "xmlDoc": true
  },

  "authors": [ "Adrian Mos" ],
  "copyright": "Adrian Mos, 2016",
  "description": "A library for transforming text expressions containing mathematical operations into executable delegates.",
  "packOptions": {
    "licenseUrl": "https://github.com/adimosh/IX.Math/blob/master/LICENSE.md",
    "owners": [ "Adrian Mos" ],
    "projectUrl": "https://github.com/adimosh/IX.Math/",
    "tags": [ "" ],
    "requireLicenseAcceptance": false
  }
}